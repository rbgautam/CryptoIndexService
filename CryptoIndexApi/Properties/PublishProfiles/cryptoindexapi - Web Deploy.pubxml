    <UserName>$cryptoindexapi</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="CryptoIndexDbContext" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:cryptoindexdb.database.windows.net,1433;Initial Catalog=CryptoIndex;Persist Security Info=True;User ID=rbgautam;Password=11@Bosejuly;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False" Name="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=(localdb)\projectsv13;Initial Catalog=CryptoIndexNew;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\CryptoIndexDbContext_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Server=(localdb)\projectsv13;Database=CryptoIndexNew;Integrated Security=True;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="CryptoIndexDbContext-Web.config Connection String">
      <ParameterValue>Data Source=tcp:cryptoindexdb.database.windows.net,1433;Initial Catalog=CryptoIndex;Persist Security Info=True;User ID=rbgautam;Password=11@Bosejuly;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
